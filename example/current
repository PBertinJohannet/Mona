data C a =
  | CI = Int -> C Int; 
  | CA = a -> C a;
  | CC = C a -> C (C a);
  | CR = f a -> C (f a);
  | CCI = C Int -> C (C Int);


let correctCA c = case c of
  (CA (CI i)) -> i,
  (CA (CA i)) -> i;

let correctCC c = case c of
  (CC (CI i)) -> i,
  (CC (CA i)) -> i;





let main = printInt 4;