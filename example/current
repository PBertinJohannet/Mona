
let main = printInt 4;


data Bool = 
    | True = Bool;
    | False = Bool;

data Prod a b = | P = a -> b -> Prod a b;
data Sum a b = 
    | Left = a -> Sum a b;
    | Right = b -> Sum a b;

data R a = 
    | RP = P a a -> R a;
    | RS = S a a -> R a;



let fail a = case a of
    (RS i) -> Left 5,
    (RP b) -> Left 1;